Algoritmo Suma_de_fracciones
	Definir n1, n2, n3, n4, t, f Como Entero;
	Escribir "ingresa un valor";
	Leer n1;
	Escribir "ingresa un valor";
	Leer n2;
	Escribir "ingresa un valor";
	Leer n3;
	Escribir "ingresa un valor";
	Leer n4;
	t<-n2*n4;
	f<-(n1*n4)+(n2*n3);
	Escribir f, "/", t;
FinAlgoritmo
